{
  "manifest_version": 2,

  "name": "RemoveAdsUpToBox",
  "description": "Single button who removed the popup AdBlock of UpToBox site.",
  "version": "1.0.3",
  "background": {"scripts": ["background.js"]},

  "permissions": ["tabs", "http://*/*"],
  "browser_action": {
    "default_icon": "icon.png",
    "default_title": "Remove Ad!!"
  }
}